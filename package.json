{
  "name": "backstage-plugin-api-linter",
  "version": "0.0.0-semantic-release",
  "description": "API Linter is a quality assurance tool that checks the compliance of API's specifications to Zalando's API rules.",
  "main": "src/index.ts",
  "packageManager": "yarn@4.10.3",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "main": "dist/index.esm.js",
    "types": "dist/index.d.ts"
  },
  "backstage": {
    "role": "frontend-plugin",
    "pluginId": "backstage-plugin-api-linter",
    "pluginPackages": [
      "backstage-plugin-api-linter"
    ]
  },
  "dependencies": {
    "@backstage/core-components": "^0.17.2",
    "@backstage/core-plugin-api": "^1.10.7",
    "@backstage/theme": "^0.6.6",
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "4.0.0-alpha.57",
    "ace-builds": "^1.4.14",
    "brace": "^0.11.1",
    "react-ace": "^9.5.0",
    "react-router-dom": "^6.4.5",
    "react-use": "^17.3.2",
    "swagger-ui-react": "^5.30.1"
  },
  "peerDependencies": {
    "@backstage/core-components": "^0.17.2",
    "@backstage/core-plugin-api": "^1.10.7",
    "@backstage/theme": "^0.6.6",
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "4.0.0-alpha.57",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@backstage/cli": "^0.32.1",
    "@backstage/core-app-api": "^1.17.0",
    "@backstage/core-components": "^0.17.2",
    "@backstage/core-plugin-api": "^1.10.7",
    "@backstage/dev-utils": "^1.1.10",
    "@backstage/test-utils": "^1.7.8",
    "@backstage/theme": "^0.6.6",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "4.0.0-alpha.57",
    "@semantic-release/github": "^10.3.5",
    "@semantic-release/npm": "^12.0.1",
    "@spotify/prettier-config": "*",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30",
    "@types/node": "*",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/swagger-ui-react": "^5.18.0",
    "commitizen": "^4.3.1",
    "concurrently": "^9.2.1",
    "cross-fetch": "^3.0.6",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^29.0.1",
    "husky": "^8.0.1",
    "msw": "^0.35.0",
    "prettier": "^3.6.2",
    "pretty-quick": "^4.2.2",
    "react": "^18",
    "react-dom": "^18",
    "semantic-release": "^25.0.1"
  },
  "sideEffects": false,
  "scripts": {
    "postinstall": "husky install",
    "build": "backstage-cli package build",
    "start": "backstage-cli package start",
    "lint": "backstage-cli package lint",
    "test": "backstage-cli package test",
    "diff": "backstage-cli plugin:diff",
    "prepack": "backstage-cli package prepack",
    "postpack": "backstage-cli package postpack",
    "clean": "backstage-cli package clean",
    "start:backend": "yarn --cwd dev start",
    "dev": "concurrently \"yarn start\" \"yarn start:backend\"",
    "tsc": "npx tsc",
    "test:ci": "backstage-cli package test --ci --no-watchman --no-watch --silent --no-cache --coverage=false",
    "format:all": "prettier \"**/*.{ts,tsx,js,jsx,json,md}\" --write",
    "format:check": "prettier \"**/*.{ts,tsx,js,jsx,json,md}\" --check",
    "format:staged": "pretty-quick --staged",
    "release": "semantic-release",
    "commit": "cz",
    "commitlint": "commitlint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zalando/backstage-plugin-api-linter.git",
    "directory": "."
  },
  "keywords": [
    "api-linter",
    "linter",
    "javascript",
    "typescript",
    "zalando",
    "API",
    "backstage",
    "plugin"
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/zalando/backstage-plugin-api-linter/issues"
  },
  "homepage": "https://github.com/zalando/backstage-plugin-api-linter#readme",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": "@spotify/prettier-config",
  "engines": {
    "node": ">=20.0.0 || >=22.0.0"
  },
  "resolutions": {
    "eslint": "8.57.0"
  }
}
